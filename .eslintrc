{
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-var": 1,
    "no-undef": 2,
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "_"
      }
    ],
    "comma-style": [
      "warn",
      "first"
    ],
    "operator-linebreak": [
      "warn",
      "before",
      {
        "overrides": {
          "=": "after"
        }
      }
    ],
    "quote-props": [
      "warn",
      "as-needed"
    ],
    "no-const-assign": [
      "warn"
    ],
    "operator-assignment": [
      "warn",
      "never"
    ],
    "no-dupe-keys": [
      "warn"
    ],
    "no-cond-assign": [
      "warn"
    ],
    "no-extra-parens": [
      "warn"
    ],
    "no-sparse-arrays": [
      "warn"
    ],
    "array-callback-return": [
      "error"
    ],
    "complexity": [
      "warn"
    ],
    "callback-return": [
      "error"
    ],
    "max-depth": [
      "warn",
      4
    ],
    "consistent-return": 2
  }
}